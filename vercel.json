{
  "redirects": [
    {
      "source": "/blog",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/blog/:slug",
      "destination": "/:slug",
      "permanent": true
    },
    {
      "source": "/blog/category/:category",
      "destination": "/category/:category",
      "permanent": true
    }
  ]
} 